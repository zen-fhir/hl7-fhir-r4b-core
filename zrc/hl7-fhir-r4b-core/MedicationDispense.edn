                         :zen.fhir/value-set {:symbol hl7-fhir-r4b-core.value-set.medicationdispense-status/value-set,
                                              :strength :required},
                         :zen/desc "preparation | in-progress | cancelled | on-hold | completed | entered-in-error | stopped | declined | unknown"},
